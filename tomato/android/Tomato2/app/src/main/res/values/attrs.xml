<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TomatoTimer">
        <attr name="state" format="integer">
        </attr>
        <attr name="states_enum" format="enum">
            <enum name="running" value="0"></enum>
            <enum name="paused"  value="1"></enum>
            <enum name="stopped" value="2"></enum>
        </attr>
    </declare-styleable>
</resources>